## SKG
## Basic coding to visualize clusters
## June 19, 2020

library(tidyverse)
devtools::load_all()

## Let's look at a cluster of size 8
data_list <- readRDS("data_output_2020-06-17_23.18.42.RDS")

sampled_data <- data_list$sampled_data

sub_data <- sampled_data %>%
    filter(cluster_size == 8, orig_id == 3)

unique_covs <- sub_data %>%
    distinct("smear", "hiv_neg_pos",
             "hiv_unk_pos", "rel_time")
