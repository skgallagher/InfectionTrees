## SKG
## June 16, 2020
## EDA on data for paper

## size, type

devtools::load_all()
library(tidyverse)
library(kable)
library(kableExtra)

data(tb_clean)

dim(tb_clean)
range(tb_clean$datecoll, na.rm = TRUE)

### grouped individuals
tb_sub <- tb_clean %>% filter(PCR.Cluster != "")
dim(tb_sub)

clust_sizes <- tb_sub %>%
    group_by(PCR.Cluster) %>%
    summarize(size = n())

tab <- t(table(clust_sizes$size))
rownames(tab) <- c("", "")

kable(t(table(clust_sizes$size)),
      format = "latex", row.names = TRUE,
      booktabs = TRUE)


